{
    "name": "mccool/laravel-auto-presenter",
    "description": "A system for auto-decorating models with presenter objects.",
    "keywords": ["lpm", "laravel", "eloquent", "presenter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn McCool",
            "email": "shawn@heybigname.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/events": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/pagination": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/view": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5|^7.5|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "McCool\\LaravelAutoPresenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McCool\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.4-dev"
        },
        "laravel": {
            "providers": [
                "McCool\\LaravelAutoPresenter\\AutoPresenterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
